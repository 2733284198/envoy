{
    "name": "laravel/envoy",
    "description": "Elegant SSH tasks for PHP.",
    "keywords": ["ssh", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~4.1 || ~5.0",
        "nategood/httpful": "~0.2",
        "symfony/console": "~3.0 || ~4.0",
        "symfony/process": "~3.0 || ~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "bin": [
        "bin/envoy"
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Envoy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Envoy\\Tests\\": "tests/"
        }
    }
}
